{
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "baseUrl": "./",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "paths": {
      "core-store": [
        "../core/src/stores"
      ],
      "core-modules": [
        "../core/src/modules"
      ],
      "core-model": [
        "../core/src/models"
      ],
      "core-params": [
        "../core/src/params"
      ],
    },
  },
  "include": [
    "src"
  ]
}